<mapper namespace="com.shadow.dao.IUserDao">

	<select id="query" parameterType="java.lang.String" resultType="com.shadow.UserEntity">
		select id ,name , password from dawn.user where name =#{name}
	</select>

	<insert id="insert" parameterType="com.shadow.UserEntity">
		insert into dawn.user
		(name,password)values (#{name},#{password})
	</insert>
</mapper>